{
  "name": "neko-ui",
  "version": "2.7.1",
  "description": "WebComponents UI Libraries",
  "main": "es/index.js",
  "scripts": {
    "build": "mo build library solid",
    "changelog": "mo changelog CHANGELOG.md && git add CHANGELOG.md",
    "lint": "mo eslint {components,site} && mo stylelint {components,site}",
    "precommit": "npm run lint && jest --coverage && npm run changelog",
    "prepare": "mo githooks pre-commit=\"npm run precommit\" commit-msg=\"npx --no -- commitlint --edit \\${1}\"",
    "start": "rm -rf lib && rm -rf es && mo start library solid",
    "test": "jest --coverage --watchAll"
  },
  "author": {
    "name": "moneko",
    "url": "https://github.com/monako97",
    "email": "meow.monako@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/monako97/neko-ui"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "neko-ui",
    "neko ui",
    "Neko UI",
    "web components",
    "Web Components",
    "webcomponents",
    "solid-js",
    "solid",
    "solid-js Components",
    "UI libraries",
    "ui libraries"
  ],
  "devDependencies": {
    "@commitlint/cli": "19.2.2",
    "@commitlint/config-conventional": "19.2.2",
    "@moneko/cli": "2.9.3",
    "@moneko/core": "3.11.2",
    "@moneko/solid": "1.7.6",
    "@solidjs/testing-library": "0.8.7",
    "@swc/jest": "0.2.36",
    "@testing-library/jest-dom": "6.4.2",
    "@types/jest": "29.5.12",
    "babel-loader": "9.1.3",
    "eslint-config-neko": "2.6.0",
    "eslint-plugin-solid": "0.13.2",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "n-code-live": "1.1.0",
    "n-katex": "1.0.8",
    "shadow-dom-testing-library": "1.11.2",
    "stylelint-config-moneko": "2.3.1"
  },
  "dependencies": {
    "@moneko/common": "1.3.2",
    "@moneko/css": "1.1.3",
    "custom-element-type": "1.0.3",
    "dayjs": "1.11.10",
    "marked-completed": "1.2.14",
    "solid-element": "1.8.0",
    "solid-js": "1.8.16"
  },
  "files": [
    "es",
    "lib",
    "umd",
    "README.md",
    "CHANGELOG.md",
    "SECURITY.md",
    "LICENSE"
  ],
  "browserslist": [
    "Chrome >= 86",
    "Safari >= 15"
  ]
}
