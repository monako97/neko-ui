{
  "name": "neko-ui",
  "version": "2.13.10",
  "description": "WebComponents UI Libraries",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "umd/index.js",
  "types": "es/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.js"
    },
    "./*": [
      "./*"
    ]
  },
  "scripts": {
    "pin": "mo pin",
    "build": "mo build library solid",
    "changelog": "mo changelog CHANGELOG.md && git add CHANGELOG.md",
    "lint:commit": "pure-lint mode=commit && npm run changelog",
    "lint:ci": "pure-lint mode=ci",
    "lint": "mo eslint [components,site] --fix",
    "postlint": "mo stylelint [components,site] --fix",
    "precommit": "npm run lint && npm run changelog",
    "prepare": "mo githooks pre-commit=\"npm run lint:commit\" commit-msg=\"commit-lint\"",
    "start": "mo start library solid",
    "test": "jest --coverage --watchAll",
    "preview": "mo server port=8080 root_dir=./docs"
  },
  "author": {
    "name": "moneko",
    "url": "https://github.com/monako97",
    "email": "meow.monako@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://monako97.github.io/neko-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/monako97/neko-ui"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "sideEffects": [
    "*.css",
    "*.less",
    "*.md",
    "@app/*"
  ],
  "keywords": [
    "neko-ui",
    "neko ui",
    "Neko UI",
    "web components",
    "Web Components",
    "webcomponents",
    "solid-js",
    "solid",
    "solid-js Components",
    "UI libraries",
    "ui libraries"
  ],
  "devDependencies": {
    "@moneko/core": "3.54.0",
    "@moneko/solid": "1.11.3",
    "@solidjs/testing-library": "0.8.10",
    "@swc/jest": "0.2.39",
    "@testing-library/jest-dom": "6.6.3",
    "@types/jest": "29.5.14",
    "@types/prismjs": "1.26.5",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "n-code-live": "2.1.1",
    "n-katex": "1.0.8",
    "shadow-dom-testing-library": "1.12.0"
  },
  "dependencies": {
    "@moneko/common": "1.10.0",
    "@moneko/css": "1.2.0",
    "component-register": "0.8.7",
    "custom-element-type": "1.0.7",
    "dayjs": "1.11.18",
    "marked-completed": "1.2.14",
    "prismjs": "1.30.0",
    "solid-element": "1.9.1",
    "solid-js": "1.9.9"
  },
  "browserslist": [
    ">0.3%, defaults",
    "fully supports es6-module",
    "maintained node versions"
  ]
}
