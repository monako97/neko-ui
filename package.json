{
  "name": "neko-ui",
  "version": "2.10.7",
  "description": "WebComponents UI Libraries",
  "main": "lib/index.js",
  "module": "es/index.js",
  "unpkg": "umd/index.js",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.js"
    },
    "./*": [
      "./*"
    ]
  },
  "scripts": {
    "build": "mo build library solid",
    "changelog": "mo changelog CHANGELOG.md && git add CHANGELOG.md",
    "lint:commit": "pure-lint mode=commit && npm run changelog",
    "lint:ci": "pure-lint mode=ci",
    "lint": "mo eslint [components,site] --fix",
    "postlint": "mo stylelint [components,site] --fix && jest --coverage",
    "precommit": "npm run lint && npm run changelog",
    "prepare": "mo githooks pre-commit=\"npm run lint:commit\" commit-msg=\"npx --no -- commitlint --edit ${1}\"",
    "start": "mo start library solid",
    "test": "jest --coverage --watchAll"
  },
  "author": {
    "name": "moneko",
    "url": "https://github.com/monako97",
    "email": "meow.monako@gmail.com"
  },
  "license": "MIT",
  "homepage": "https://monako97.github.io/neko-ui",
  "repository": {
    "type": "git",
    "url": "https://github.com/monako97/neko-ui"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "sideEffects": [
    "*.css",
    "*.less",
    "*.md",
    "@app/*"
  ],
  "keywords": [
    "neko-ui",
    "neko ui",
    "Neko UI",
    "web components",
    "Web Components",
    "webcomponents",
    "solid-js",
    "solid",
    "solid-js Components",
    "UI libraries",
    "ui libraries"
  ],
  "devDependencies": {
    "@commitlint/cli": "19.8.0",
    "@commitlint/config-conventional": "19.8.0",
    "@moneko/core": "3.40.4",
    "@moneko/eslint": "1.3.0",
    "@moneko/solid": "1.11.1",
    "@moneko/stylelint": "1.3.0",
    "@solidjs/testing-library": "0.8.10",
    "@swc/jest": "0.2.37",
    "@testing-library/jest-dom": "6.6.3",
    "@types/jest": "29.5.14",
    "@types/prismjs": "1.26.5",
    "husky": "9.1.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "marked-completed": "1.2.14",
    "n-code-live": "2.0.1",
    "n-katex": "1.0.8",
    "shadow-dom-testing-library": "1.12.0"
  },
  "dependencies": {
    "@moneko/common": "1.7.7",
    "@moneko/css": "1.2.0",
    "custom-element-type": "1.0.6",
    "dayjs": "1.11.13",
    "prismjs": "1.30.0",
    "solid-element": "1.8.1",
    "solid-js": "1.8.23"
  },
  "browserslist": [
    ">0.3%, defaults",
    "fully supports es6-module",
    "maintained node versions"
  ]
}
