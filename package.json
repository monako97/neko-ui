{
  "name": "neko-ui",
  "version": "2.8.7",
  "description": "WebComponents UI Libraries",
  "main": "lib/index.js",
  "module": "es/index.js",
  "types": "types/index.d.ts",
  "exports": {
    ".": {
      "require": "./lib/index.js",
      "import": "./es/index.js"
    },
    "./*": [
      "./*"
    ]
  },
  "scripts": {
    "build": "mo build library solid",
    "changelog": "mo changelog CHANGELOG.md && git add CHANGELOG.md",
    "lint": "mo eslint components && mo eslint site",
    "postlint": "mo stylelint components && mo stylelint site && jest --coverage",
    "precommit": "npm run lint && npm run changelog",
    "prepare": "mo githooks pre-commit=\"npm run precommit\" commit-msg=\"npx --no -- commitlint --edit \\${1}\"",
    "start": "rm -rf lib && rm -rf es && mo start library solid",
    "test": "jest --coverage --watchAll"
  },
  "author": {
    "name": "moneko",
    "url": "https://github.com/monako97",
    "email": "meow.monako@gmail.com"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/monako97/neko-ui"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org"
  },
  "keywords": [
    "neko-ui",
    "neko ui",
    "Neko UI",
    "web components",
    "Web Components",
    "webcomponents",
    "solid-js",
    "solid",
    "solid-js Components",
    "UI libraries",
    "ui libraries"
  ],
  "devDependencies": {
    "@commitlint/cli": "19.3.0",
    "@commitlint/config-conventional": "19.2.2",
    "@moneko/cli": "2.13.5",
    "@moneko/core": "3.17.16",
    "@moneko/solid": "1.8.0",
    "@solidjs/testing-library": "0.8.8",
    "@swc/jest": "0.2.36",
    "@testing-library/jest-dom": "6.4.5",
    "@types/jest": "29.5.12",
    "@types/prismjs": "1.26.4",
    "eslint-config-neko": "2.7.6",
    "eslint-plugin-solid": "0.14.0",
    "husky": "9.0.11",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "n-code-live": "1.1.0",
    "n-katex": "1.0.8",
    "shadow-dom-testing-library": "1.11.2",
    "stylelint-config-moneko": "2.4.0"
  },
  "dependencies": {
    "@moneko/common": "1.5.3",
    "@moneko/css": "1.1.5",
    "custom-element-type": "1.0.4",
    "dayjs": "1.11.11",
    "marked-completed": "1.2.14",
    "prismjs": "1.29.0",
    "solid-element": "1.8.0",
    "solid-js": "1.8.17"
  },
  "browserslist": [
    ">0.3%, defaults",
    "fully supports es6-module",
    "maintained node versions"
  ]
}
